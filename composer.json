{
  "name": "apie/faker",
  "description": "Composer package of the apie library: faker",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\Faker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\Faker\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "apie/core": "self.version",
    "fakerphp/faker": "^1.23.1",
    "guzzlehttp/psr7": "^2.4",
    "maennchen/zipstream-php": "^3.0",
    "niklongstone/regex-reverse": "^0.4.0|^0.5.0",
    "symfony/console": "^6.4"
  },
  "require-dev": {
    "apie/common-value-objects": "self.version",
    "apie/date-value-objects": "self.version",
    "apie/fixtures": "self.version",
    "apie/service-provider-generator": "0.11.2",
    "apie/text-value-objects": "self.version",
    "illuminate/support": "*",
    "phpunit/phpunit": "^9.6.19",
    "phpspec/prophecy-phpunit": "^2.2",
    "symfony/finder": "^6.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}